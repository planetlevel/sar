{
  "$schema": "../schema/framework-schema.json",
  "name": "go-openai",
  "languages": [
    "go"
  ],
  "detection": {
    "dependencies": {
      "go.mod": [
        {
          "pattern": "github.com/sashabaranov/go-openai",
          "library": "go-openai"
        }
      ]
    }
  },
  "architecture": {
    "integration": {
      "ai_services": [
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "Client.CreateChatCompletion"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "Client.CreateCompletion"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "Client.CreateEmbedding"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "Client.CreateImage"
        }
      ]
    }
  }
}