{
  "$schema": "../schema/framework-schema.json",
  "name": "Spring Context",
  "languages": [
    "java",
    "kotlin"
  ],
  "repository": {
    "url": "https://github.com/spring-projects/spring-framework",
    "branch": "main",
    "commit": "5e213b2407bc211aee4bfa02f5e1909128860f2f",
    "date": "2025-11-04"
  },
  "detection": {
    "files": {
      "target": "filename",
      "pattern": [
        "spring-context*.jar"
      ]
    },
    "dependencies": {},
    "imports": {
      "search_type": "import",
      "target": "joern",
      "pattern": [
        "org.springframework.context.*",
        "org.springframework.scheduling.*",
        "org.springframework.jmx.*",
        "org.springframework.cache.*",
        "org.springframework.validation.*"
      ]
    },
    "package_hierarchy": {
      "description": "Packages used in the framework organized by depth level",
      "levels": {
        "depth_2": [
          "org.springframework"
        ],
        "depth_3": [
          "org.springframework.context",
          "org.springframework.scheduling",
          "org.springframework.cache",
          "org.springframework.format",
          "org.springframework.jmx",
          "org.springframework.validation",
          "org.springframework.scripting",
          "org.springframework.jndi",
          "org.springframework.instrument",
          "org.springframework.resilience",
          "org.springframework.ejb",
          "org.springframework.stereotype",
          "org.springframework.ui"
        ],
        "depth_4": [
          "org.springframework.context.annotation",
          "org.springframework.format.datetime",
          "org.springframework.jmx.export",
          "org.springframework.cache.interceptor",
          "org.springframework.context.support",
          "org.springframework.scheduling.concurrent",
          "org.springframework.context.event",
          "org.springframework.validation.beanvalidation",
          "org.springframework.scheduling.support",
          "org.springframework.scheduling.config",
          "org.springframework.scheduling.annotation",
          "org.springframework.context.aot",
          "org.springframework.cache.annotation",
          "org.springframework.instrument.classloading",
          "org.springframework.jmx.support",
          "org.springframework.context.expression",
          "org.springframework.format.number",
          "org.springframework.format.support",
          "org.springframework.jmx.access",
          "org.springframework.resilience.annotation"
        ]
      },
      "target": "analysis"
    }
  },
  "architecture": {
    "execution": {
      "reflection": [
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.scripting.bsh.BshScriptUtils$BshObjectInvocationHandler.invoke:java.lang.Object(java.lang.Object,java.lang.reflect.Method,java.lang.Object[])",
          "description": "Uses reflection to invoke methods on BeanShell script objects"
        },
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.context.support.GenericGroovyApplicationContext.invokeMethod:java.lang.Object(java.lang.String,java.lang.Object)",
          "description": "Dynamically invokes methods using Groovy's method invocation mechanism"
        },
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.cache.annotation.CacheAnnotationParser.parseCacheAnnotations:java.util.Collection(java.lang.reflect.Method)",
          "description": "Uses reflection to parse cache annotations on methods"
        },
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.cache.annotation.CacheAnnotationParser.parseCacheAnnotations:java.util.Collection(java.lang.Class)",
          "description": "Uses reflection to parse cache annotations on classes"
        }
      ],
      "expression": [
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.context.expression.StandardBeanExpressionResolver.evaluate:java.lang.Object(java.lang.String,org.springframework.beans.factory.config.BeanExpressionContext)",
          "description": "Evaluates Spring Expression Language (SpEL) expressions in bean definitions"
        },
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.scripting.ScriptEvaluator.evaluate:java.lang.Object(org.springframework.scripting.ScriptSource)",
          "description": "Evaluates script source code and returns the result"
        },
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.scripting.bsh.BshScriptEvaluator.evaluate:java.lang.Object(org.springframework.scripting.ScriptSource)",
          "description": "Evaluates BeanShell scripts and returns the result"
        },
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.scripting.groovy.GroovyScriptEvaluator.evaluate:java.lang.Object(org.springframework.scripting.ScriptSource)",
          "description": "Evaluates Groovy scripts and returns the result"
        },
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.format.support.FormattingConversionService$PrinterConverter.convert:java.lang.Object(java.lang.Object,org.springframework.core.convert.TypeDescriptor,org.springframework.core.convert.TypeDescriptor)",
          "description": "Converts objects between types using formatting expressions"
        },
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.format.support.FormattingConversionService$ParserConverter.convert:java.lang.Object(java.lang.Object,org.springframework.core.convert.TypeDescriptor,org.springframework.core.convert.TypeDescriptor)",
          "description": "Parses and converts objects using formatting expressions"
        }
      ]
    },
    "defense": {
      "input_validation": [
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.validation.ValidationUtils.invokeValidator:void(org.springframework.validation.Validator,java.lang.Object,org.springframework.validation.Errors)",
          "description": "Invokes a validator to validate an object and collect validation errors"
        },
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.validation.beanvalidation.MethodValidationInterceptor.invoke:java.lang.Object(org.aopalliance.intercept.MethodInvocation)",
          "description": "Intercepts method calls to perform bean validation"
        }
      ]
    },
    "logging": {
      "operations": [
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.scheduling.support.DelegatingErrorHandlingRunnable.run:void()",
          "description": "Executes a runnable with error handling and logging"
        }
      ]
    },
    "integration": {
      "messaging": [
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.context.event.EventPublicationInterceptor.invoke:java.lang.Object(org.aopalliance.intercept.MethodInvocation)",
          "description": "Intercepts method calls to publish events to the application context"
        }
      ]
    },
    "data_flow": {
      "input_sources": [
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.format.Parser.parse:java.lang.Object(java.lang.String,java.util.Locale)",
          "description": "Parses input string into an object using locale-specific formatting"
        }
      ],
      "serialization": [
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.format.datetime.DateFormatter.parse:java.util.Date(java.lang.String,java.util.Locale)",
          "description": "Converts string representation to Date object using locale-specific formatting"
        },
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.format.datetime.standard.InstantFormatter.parse:java.time.Instant(java.lang.String,java.util.Locale)",
          "description": "Converts string representation to Instant object using locale-specific formatting"
        }
      ],
      "xml_parsing": [
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.cache.config.AnnotationDrivenCacheBeanDefinitionParser.parse:org.springframework.beans.factory.config.BeanDefinition(org.w3c.dom.Element,org.springframework.beans.factory.xml.ParserContext)",
          "description": "Parses XML configuration for cache annotations"
        },
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.context.annotation.AnnotationConfigBeanDefinitionParser.parse:org.springframework.beans.factory.config.BeanDefinition(org.w3c.dom.Element,org.springframework.beans.factory.xml.ParserContext)",
          "description": "Parses XML configuration for annotation-based context"
        },
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.context.annotation.ComponentScanBeanDefinitionParser.parse:org.springframework.beans.factory.config.BeanDefinition(org.w3c.dom.Element,org.springframework.beans.factory.xml.ParserContext)",
          "description": "Parses XML configuration for component scanning"
        }
      ]
    }
  }
}
