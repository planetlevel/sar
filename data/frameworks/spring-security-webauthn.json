          "search_type": "annotation_name",
          "pattern": "org.springframework.security.web.webauthn.management.JdbcUserCredentialRepository.delete:void(org.springframework.security.web.webauthn.api.Bytes)",
          "description": "Repository method to delete a credential record by ID"
