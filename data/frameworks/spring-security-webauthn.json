        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.security.web.webauthn.api.PublicKeyCredentialRequestOptions$PublicKeyCredentialRequestOptionsBuilder.build:org.springframework.security.web.webauthn.api.PublicKeyCredentialRequestOptions():org.springframework.security.web.webauthn.api.PublicKeyCredentialRequestOptions()",
          "description": "Builds WebAuthn credential request options object"
