          "pattern": "org.springframework.security.web.webauthn.management.JdbcUserCredentialRepository.save:void(org.springframework.security.web.webauthn.api.CredentialRecord)",
          "description": "Repository method to save a credential record"
        },
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.security.web.webauthn.management.JdbcPublicKeyCredentialUserEntityRepository.delete:void(org.springframework.security.web.webauthn.api.Bytes)",
