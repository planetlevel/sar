        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.security.web.webauthn.authentication.WebAuthnAuthentication$Builder.build:org.springframework.security.web.webauthn.authentication.WebAuthnAuthentication():org.springframework.security.web.webauthn.authentication.WebAuthnAuthentication()",
          "description": "Builds a WebAuthn authentication object for web authentication security"
