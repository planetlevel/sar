{
  "$schema": "../schema/framework-schema.json",
  "name": "Anthropic Python SDK",
  "languages": [
    "python"
  ],
  "extends": "python",
  "detection": {
    "dependencies": {
      "requirements.txt": [
        {
          "pattern": "anthropic",
          "library": "Anthropic Python SDK"
        }
      ],
      "pyproject.toml": [
        {
          "pattern": "anthropic",
          "library": "Anthropic Python SDK"
        }
      ]
    }
  },
  "architecture": {
    "integration": {
      "ai_services": [
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "anthropic.py:<module>.Anthropic.__init__.<returnValue>.<member>(messages).create"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "anthropic.py:<module>.Anthropic.__init__.<returnValue>.<member>(messages).stream"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "anthropic.py:<module>.AsyncAnthropic.__init__.<returnValue>.<member>(messages).create"
        }
      ]
    }
  }
}