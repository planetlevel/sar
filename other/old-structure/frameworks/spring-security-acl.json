{
  "$schema": "../schema/framework-schema.json",
  "name": "spring-security-acl",
  "languages": [
    "java"
  ],
  "repository": {
    "url": "https://github.com/spring-projects/spring-security",
    "branch": "main",
    "commit": "5662e173706d437bcc23b65466abdb18733db277",
    "date": "2025-11-04"
  },
  "detection": {
    "files": {
      "target": "filename",
      "pattern": [
        "spring-security-acl*.jar"
      ]
    },
    "dependencies": {},
    "imports": {
      "search_type": "import",
      "target": "joern",
      "pattern": [
        "org.springframework.*",
        "lock.GlobalLockPlugin.*",
        "lock.GlobalLockTask.*",
        "s101.S101Configure.*",
        "s101.S101Configurer.*"
      ]
    },
    "package_hierarchy": {
      "description": "Packages used in the framework organized by depth level",
      "levels": {
        "depth_2": [
          "org.springframework",
          "lock.GlobalLockTask",
          "lock.GlobalLockPlugin",
          "s101.S101Configure",
          "s101.S101Configurer",
          "s101.S101Install",
          "s101.S101Plugin",
          "s101.S101PluginExtension",
          "trang.RncToXsd",
          "trang.TrangPlugin"
        ],
        "depth_3": [
          "org.springframework.security",
          "org.springframework.gradle",
          "lock.GlobalLockTask.lock",
          "lock.GlobalLockTask.writeLocksFor"
        ],
        "depth_4": [
          "org.springframework.security.oauth2",
          "org.springframework.security.web",
          "org.springframework.security.config",
          "org.springframework.security.saml2",
          "org.springframework.security.authorization",
          "org.springframework.security.authentication",
          "org.springframework.security.core",
          "org.springframework.security.access",
          "org.springframework.security.ldap",
          "org.springframework.security.acls",
          "org.springframework.security.crypto",
          "org.springframework.security.messaging",
          "org.springframework.security.kerberos",
          "org.springframework.security.rsocket",
          "org.springframework.security.cas",
          "org.springframework.security.jackson2",
          "org.springframework.security.jackson",
          "org.springframework.gradle.maven",
          "org.springframework.security.integration",
          "org.springframework.security.aot"
        ]
      },
      "target": "analysis"
    }
  },
  "architecture": {
    "database": {
      "sql_queries": [
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.security.acls.jdbc.JdbcAclService.findChildren:java.util.List(org.springframework.security.acls.model.ObjectIdentity)",
          "description": "Executes SQL query to find child ACL entries for a given parent identity"
        },
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.security.acls.jdbc.JdbcMutableAclService.deleteAcl:void(org.springframework.security.acls.model.ObjectIdentity,boolean)",
          "description": "Executes SQL delete operations to remove ACL entries from database"
        },
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.security.acls.jdbc.JdbcMutableAclService.updateAcl:org.springframework.security.acls.model.MutableAcl(org.springframework.security.acls.model.MutableAcl)",
          "description": "Executes SQL update operations to modify ACL entries in database"
        },
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.security.acls.jdbc.JdbcMutableAclService.createAcl:org.springframework.security.acls.model.MutableAcl(org.springframework.security.acls.model.ObjectIdentity)",
          "description": "Executes SQL insert operations to create new ACL entries in database"
        }
      ]
    },
    "defense": {
      "authorization": [
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.security.acls.domain.AclImpl.deleteAce:void(int)",
          "description": "Removes an access control entry affecting authorization decisions"
        },
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.security.acls.model.MutableAcl.deleteAce:void(int)",
          "description": "Interface method for removing an access control entry"
        },
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.security.acls.domain.AclImpl.insertAce:void(int,org.springframework.security.acls.model.Permission,org.springframework.security.acls.model.Sid,boolean)",
          "description": "Adds a new access control entry for authorization decisions"
        },
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.security.acls.model.MutableAcl.insertAce:void(int,org.springframework.security.acls.model.Permission,org.springframework.security.acls.model.Sid,boolean)",
          "description": "Interface method for adding a new access control entry"
        },
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.security.acls.domain.AclImpl.updateAce:void(int,org.springframework.security.acls.model.Permission)",
          "description": "Modifies an existing access control entry's permissions"
        },
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.security.acls.model.MutableAcl.updateAce:void(int,org.springframework.security.acls.model.Permission)",
          "description": "Interface method for modifying an access control entry's permissions"
        }
      ]
    },
    "logging": {
      "operations": [
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.security.acls.domain.AclImpl.updateAuditing:void(int,boolean,boolean)",
          "description": "Updates auditing flags for security-related logging"
        },
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.security.acls.model.AuditableAcl.updateAuditing:void(int,boolean,boolean)",
          "description": "Interface method for updating security audit logging settings"
        }
      ]
    },
    "data_flow": {
      "serialization": [
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.security.acls.domain.SpringCacheBasedAclCache.getFromCache:org.springframework.security.acls.model.MutableAcl(java.io.Serializable)",
          "description": "Retrieves serialized ACL data from cache using primary key"
        },
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.security.acls.model.AclCache.getFromCache:org.springframework.security.acls.model.MutableAcl(java.io.Serializable)",
          "description": "Interface method for retrieving serialized ACL data from cache"
        }
      ]
    }
  }
}
