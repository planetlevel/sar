{
  "$schema": "../schema/framework-schema.json",
  "name": "OpenAI Java Client",
  "languages": [
    "java"
  ],
  "extends": "java",
  "detection": {
    "dependencies": {
      "pom.xml": [
        {
          "artifact": "openai-java",
          "library": "OpenAI Java Client"
        }
      ],
      "build.gradle": [
        {
          "pattern": "com.openai",
          "library": "OpenAI"
        }
      ]
    }
  },
  "architecture": {
    "integration": {
      "ai_services": [
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "com.openai.client.OpenAIClient.chat:com.openai.models.ChatCompletion(com.openai.models.ChatCompletionCreateParams)"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "com.openai.client.OpenAIClient.completions:com.openai.models.Completion(com.openai.models.CompletionCreateParams)"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "com.openai.client.OpenAIClient.embeddings:com.openai.models.Embedding(com.openai.models.EmbeddingCreateParams)"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "com.openai.client.OpenAIClient.images:com.openai.models.Image(com.openai.models.ImageCreateParams)"
        }
      ]
    }
  }
}