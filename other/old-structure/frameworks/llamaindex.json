{
  "$schema": "../schema/framework-schema.json",
  "name": "LlamaIndex",
  "languages": [
    "python"
  ],
  "extends": "python",
  "detection": {
    "dependencies": {
      "requirements.txt": [
        {
          "pattern": "llama-index",
          "library": "LlamaIndex"
        }
      ],
      "pyproject.toml": [
        {
          "pattern": "llama-index",
          "library": "LlamaIndex"
        }
      ]
    }
  },
  "architecture": {
    "integration": {
      "ai_services": [
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "llama_index.py:<module>.QueryEngine.__init__.<returnValue>.query"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "llama_index.py:<module>.ChatEngine.__init__.<returnValue>.chat"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "llama_index.py:<module>.VectorStoreIndex.__init__.<returnValue>.as_query_engine"
        }
      ]
    }
  }
}