{
  "$schema": "../schema/framework-schema.json",
  "name": "LangChain.js",
  "languages": [
    "javascript",
    "typescript"
  ],
  "detection": {
    "dependencies": {
      "package.json": [
        {
          "pattern": "\"langchain\"",
          "library": "LangChain.js"
        }
      ]
    }
  },
  "architecture": {
    "integration": {
      "ai_services": [
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "ChatOpenAI.call"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "OpenAI.call"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "LLMChain.call"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "OpenAIEmbeddings.embedQuery"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "AgentExecutor.call"
        }
      ]
    }
  }
}