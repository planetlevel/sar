{
  "$schema": "../schema/framework-schema.json",
  "name": "Vercel AI SDK",
  "languages": [
    "javascript",
    "typescript"
  ],
  "detection": {
    "dependencies": {
      "package.json": [
        {
          "pattern": "\"ai\"",
          "library": "Vercel AI SDK"
        }
      ]
    }
  },
  "architecture": {
    "integration": {
      "ai_services": [
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "generateText"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "streamText"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "generateObject"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "embed"
        }
      ]
    }
  }
}