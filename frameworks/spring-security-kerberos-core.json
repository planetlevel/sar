{
  "$schema": "../schema/framework-schema.json",
  "name": "spring-security-kerberos-core",
  "languages": [
    "java"
  ],
  "repository": {
    "url": "https://github.com/spring-projects/spring-security",
    "branch": "main",
    "commit": "5662e173706d437bcc23b65466abdb18733db277",
    "date": "2025-11-04"
  },
  "detection": {
    "files": {
      "type": "filename",
      "patterns": [
        "spring-security-kerberos-core*.jar"
      ]
    },
    "dependencies": {},
    "imports": {
      "type": "joern",
      "search_type": "import",
      "patterns": [
        "org.springframework.*",
        "lock.GlobalLockPlugin.*",
        "lock.GlobalLockTask.*",
        "s101.S101Configure.*",
        "s101.S101Configurer.*"
      ]
    },
    "package_hierarchy": {
      "type": "analysis",
      "description": "Packages used in the framework organized by depth level",
      "levels": {
        "depth_2": [
          "org.springframework",
          "lock.GlobalLockTask",
          "lock.GlobalLockPlugin",
          "s101.S101Configure",
          "s101.S101Configurer",
          "s101.S101Install",
          "s101.S101Plugin",
          "s101.S101PluginExtension",
          "trang.RncToXsd",
          "trang.TrangPlugin"
        ],
        "depth_3": [
          "org.springframework.security",
          "org.springframework.gradle",
          "lock.GlobalLockTask.lock",
          "lock.GlobalLockTask.writeLocksFor"
        ],
        "depth_4": [
          "org.springframework.security.oauth2",
          "org.springframework.security.web",
          "org.springframework.security.config",
          "org.springframework.security.saml2",
          "org.springframework.security.authorization",
          "org.springframework.security.authentication",
          "org.springframework.security.core",
          "org.springframework.security.access",
          "org.springframework.security.ldap",
          "org.springframework.security.acls",
          "org.springframework.security.crypto",
          "org.springframework.security.messaging",
          "org.springframework.security.kerberos",
          "org.springframework.security.rsocket",
          "org.springframework.security.cas",
          "org.springframework.security.jackson2",
          "org.springframework.security.jackson",
          "org.springframework.gradle.maven",
          "org.springframework.security.integration",
          "org.springframework.security.aot"
        ]
      }
    }
  },
  "architecture": {
    "defense": {
      "authentication": {
        "annotations": {
          "type": "joern",
          "search_type": "annotation_name",
          "patterns": [
            {
              "signature": "org.springframework.security.kerberos.authentication.KerberosAuthenticationProvider.authenticate:org.springframework.security.core.Authentication(org.springframework.security.core.Authentication)",
              "description": "Authenticates users using Kerberos credentials"
            },
            {
              "signature": "org.springframework.security.kerberos.authentication.KerberosMultiTier.authenticateService:org.springframework.security.core.Authentication(org.springframework.security.core.Authentication,java.lang.String,int,java.lang.String)",
              "description": "Performs multi-tier Kerberos service authentication"
            },
            {
              "signature": "org.springframework.security.kerberos.authentication.KerberosServiceAuthenticationProvider.authenticate:org.springframework.security.core.Authentication(org.springframework.security.core.Authentication)",
              "description": "Authenticates Kerberos service tickets"
            },
            {
              "signature": "org.springframework.security.kerberos.authentication.KerberosTicketValidator.validateTicket:org.springframework.security.kerberos.authentication.KerberosTicketValidation(byte[])",
              "description": "Validates Kerberos tickets for authentication"
            },
            {
              "signature": "org.springframework.security.kerberos.authentication.sun.SunJaasKerberosTicketValidator.validateTicket:org.springframework.security.kerberos.authentication.KerberosTicketValidation(byte[])",
              "description": "Validates Kerberos tickets using Sun JAAS implementation"
            }
          ]
        }
      },
      "crypto": {
        "annotations": {
          "type": "joern",
          "search_type": "annotation_name",
          "patterns": [
            {
              "signature": "org.springframework.security.kerberos.authentication.KerberosServiceRequestToken.encrypt:byte[](byte[],int,int)",
              "description": "Encrypts data using Kerberos session keys"
            },
            {
              "signature": "org.springframework.security.kerberos.authentication.KerberosServiceRequestToken.encrypt:byte[](byte[])",
              "description": "Encrypts data using Kerberos session keys with default offset and length"
            },
            {
              "signature": "org.springframework.security.kerberos.authentication.KerberosServiceRequestToken.decrypt:byte[](byte[],int,int)",
              "description": "Decrypts data using Kerberos session keys"
            },
            {
              "signature": "org.springframework.security.kerberos.authentication.KerberosServiceRequestToken.decrypt:byte[](byte[])",
              "description": "Decrypts data using Kerberos session keys with default offset and length"
            }
          ]
        }
      }
    },
    "execution": {
      "native": {
        "annotations": {
          "type": "joern",
          "search_type": "annotation_name",
          "patterns": [
            {
              "signature": "org.springframework.security.kerberos.authentication.KerberosMultiTier.authenticateService.PrivilegedAction$0.run:java.lang.Object()",
              "description": "Executes privileged Kerberos authentication action"
            },
            {
              "signature": "org.springframework.security.kerberos.authentication.sun.SunJaasKerberosTicketValidator$KerberosMultitierValidateAction.run:org.springframework.security.kerberos.authentication.KerberosTicketValidation()",
              "description": "Executes multi-tier Kerberos ticket validation"
            },
            {
              "signature": "org.springframework.security.kerberos.authentication.sun.SunJaasKerberosTicketValidator$KerberosValidateAction.run:org.springframework.security.kerberos.authentication.KerberosTicketValidation()",
              "description": "Executes Kerberos ticket validation"
            }
          ]
        }
      }
    }
  }
}