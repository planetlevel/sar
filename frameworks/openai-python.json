{
  "$schema": "../schema/framework-schema.json",
  "name": "OpenAI Python SDK",
  "languages": [
    "python"
  ],
  "extends": "python",
  "detection": {
    "dependencies": {
      "requirements.txt": [
        {
          "pattern": "openai",
          "library": "OpenAI Python SDK"
        }
      ],
      "pyproject.toml": [
        {
          "pattern": "openai",
          "library": "OpenAI Python SDK"
        }
      ]
    }
  },
  "architecture": {
    "integration": {
      "ai_services": {
        "type": "joern",
        "search_type": "method_signature",
        "signatures": [
          "openai.py:<module>.OpenAI.__init__.<returnValue>.<member>(chat).<member>(completions).create",
          "openai.py:<module>.OpenAI.__init__.<returnValue>.<member>(completions).create",
          "openai.py:<module>.OpenAI.__init__.<returnValue>.<member>(embeddings).create",
          "openai.py:<module>.OpenAI.__init__.<returnValue>.<member>(images).generate",
          "openai.py:<module>.AsyncOpenAI.__init__.<returnValue>.<member>(chat).<member>(completions).create"
        ]
      }
    }
  }
}