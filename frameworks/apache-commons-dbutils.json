{
  "name": "Apache Commons DbUtils",
  "languages": [
    "java"
  ],
  "detection": {
    "files": {
      "type": "filename",
      "patterns": []
    },
    "dependencies": {
      "pom.xml": [
        {
          "artifact": "commons-parent",
          "library": "Apache Commons DbUtils"
        }
      ]
    },
    "imports": {
      "type": "joern",
      "search_type": "import",
      "patterns": [
        "java.sql.*",
        "java.util.*",
        "org.apache.commons.dbutils.*",
        "java.lang.reflect.*",
        "java.beans.*",
        "javax.sql.*",
        "java.io.*",
        "java.math.*",
        "java.net.*",
        "java.util.concurrent.*"
      ]
    }
  },
  "architecture": {
    "database": {
      "sql_queries": {
        "type": "joern",
        "search_type": "method_signature",
        "signatures": [
          {
            "signature": "org.apache.commons.dbutils.QueryRunner.batch:int[](java.sql.Connection,java.lang.String,java.lang.Object[][])",
            "description": "Executes a batch of SQL statements with parameters"
          },
          {
            "signature": "org.apache.commons.dbutils.QueryRunner.query:java.lang.Object(java.sql.Connection,java.lang.String,org.apache.commons.dbutils.ResultSetHandler)",
            "description": "Executes a SQL SELECT query and processes results through a handler"
          },
          {
            "signature": "org.apache.commons.dbutils.QueryRunner.update:int(java.sql.Connection,java.lang.String)",
            "description": "Executes a SQL INSERT, UPDATE, or DELETE statement"
          },
          {
            "signature": "org.apache.commons.dbutils.QueryRunner.insert:java.lang.Object(java.sql.Connection,java.lang.String,org.apache.commons.dbutils.ResultSetHandler)",
            "description": "Executes a SQL INSERT statement and returns generated keys"
          }
        ]
      },
      "orm_operations": {
        "type": "joern",
        "search_type": "method_signature",
        "signatures": [
          {
            "signature": "org.apache.commons.dbutils.BasicRowProcessor.toBean:java.lang.Object(java.sql.ResultSet,java.lang.Class)",
            "description": "Maps a ResultSet row to a Java bean object"
          },
          {
            "signature": "org.apache.commons.dbutils.BasicRowProcessor.toBeanList:java.util.List(java.sql.ResultSet,java.lang.Class)",
            "description": "Maps multiple ResultSet rows to a list of Java beans"
          },
          {
            "signature": "org.apache.commons.dbutils.BeanProcessor.populateBean:java.lang.Object(java.sql.ResultSet,java.lang.Object)",
            "description": "Populates a bean's properties from a ResultSet row"
          }
        ]
      },
      "repository_pattern": {
        "type": "joern",
        "search_type": "method_signature",
        "signatures": [
          {
            "signature": "org.apache.commons.dbutils.QueryLoader.load:java.util.Map(java.lang.String)",
            "description": "Loads SQL queries from a properties file into a reusable repository"
          }
        ]
      }
    },
    "execution": {
      "reflection": {
        "type": "joern",
        "search_type": "method_signature",
        "signatures": [
          {
            "signature": "org.apache.commons.dbutils.wrappers.SqlNullCheckedResultSet.invoke:java.lang.Object(java.lang.Object,java.lang.reflect.Method,java.lang.Object[])",
            "description": "Dynamically invokes methods on a ResultSet proxy for null checking"
          },
          {
            "signature": "org.apache.commons.dbutils.wrappers.StringTrimmedResultSet.invoke:java.lang.Object(java.lang.Object,java.lang.reflect.Method,java.lang.Object[])",
            "description": "Dynamically invokes methods on a ResultSet proxy for string trimming"
          }
        ]
      }
    },
    "data_flow": {
      "serialization": {
        "type": "joern",
        "search_type": "method_signature",
        "signatures": [
          {
            "signature": "org.apache.commons.dbutils.BasicRowProcessor.toMap:java.util.Map(java.sql.ResultSet)",
            "description": "Converts a ResultSet row to a Map representation"
          },
          {
            "signature": "org.apache.commons.dbutils.BasicRowProcessor.toArray:java.lang.Object[](java.sql.ResultSet)",
            "description": "Converts a ResultSet row to an array representation"
          }
        ]
      }
    }
  }
}