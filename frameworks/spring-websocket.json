{
  "$schema": "../schema/framework-schema.json",
  "name": "Spring WebSocket",
  "languages": [
    "java"
  ],
  "repository": {
    "url": "https://github.com/spring-projects/spring-framework",
    "branch": "main",
    "commit": "5e213b2407bc211aee4bfa02f5e1909128860f2f",
    "date": "2025-11-04"
  },
  "detection": {
    "files": {
      "type": "filename",
      "patterns": [
        "spring-websocket*.jar"
      ]
    },
    "dependencies": {},
    "imports": {
      "type": "joern",
      "search_type": "import",
      "patterns": [
        "org.springframework.web.socket.sockjs.*",
        "org.springframework.web.socket.config.*",
        "org.springframework.web.socket.server.*",
        "org.springframework.web.socket.messaging.*",
        "org.springframework.web.socket.adapter.*"
      ]
    },
    "package_hierarchy": {
      "type": "analysis",
      "description": "Packages used in the framework organized by depth level",
      "levels": {
        "depth_2": [
          "org.springframework"
        ],
        "depth_3": [
          "org.springframework.web"
        ],
        "depth_4": [
          "org.springframework.web.socket"
        ]
      }
    }
  },
  "architecture": {
    "communication": {
      "socket": {
        "annotations": {
          "type": "joern",
          "search_type": "annotation_name",
          "patterns": [
            "org.springframework.web.socket.WebSocketSession.sendMessage:void(org.springframework.web.socket.WebSocketMessage)",
            "org.springframework.web.socket.adapter.AbstractWebSocketSession.sendMessage:void(org.springframework.web.socket.WebSocketMessage)",
            "org.springframework.web.socket.handler.ConcurrentWebSocketSessionDecorator.sendMessage:void(org.springframework.web.socket.WebSocketMessage)"
          ]
        }
      }
    },
    "integration": {
      "messaging": {
        "annotations": {
          "type": "joern",
          "search_type": "annotation_name",
          "patterns": [
            "org.springframework.web.socket.messaging.WebSocketStompClient$WebSocketTcpConnectionHandlerAdapter.sendAsync:java.util.concurrent.CompletableFuture(org.springframework.messaging.Message)"
          ]
        }
      }
    },
    "data_flow": {
      "serialization": {
        "annotations": {
          "type": "joern",
          "search_type": "annotation_name",
          "patterns": [
            "org.springframework.web.socket.adapter.standard.ConvertingEncoderDecoderSupport.encode:java.lang.Object(java.lang.Object)",
            "org.springframework.web.socket.adapter.standard.ConvertingEncoderDecoderSupport.decode:java.lang.Object(java.lang.Object)",
            "org.springframework.web.socket.sockjs.frame.Jackson2SockJsMessageCodec.decode:java.lang.String[](java.lang.String)"
          ]
        }
      },
      "xml_parsing": {
        "annotations": {
          "type": "joern",
          "search_type": "annotation_name",
          "patterns": [
            "org.springframework.web.socket.config.HandlersBeanDefinitionParser.parse:org.springframework.beans.factory.config.BeanDefinition(org.w3c.dom.Element,org.springframework.beans.factory.xml.ParserContext)",
            "org.springframework.web.socket.config.MessageBrokerBeanDefinitionParser.parse:org.springframework.beans.factory.config.BeanDefinition(org.w3c.dom.Element,org.springframework.beans.factory.xml.ParserContext)"
          ]
        }
      }
    },
    "presentation": {
      "headers": {
        "annotations": {
          "type": "joern",
          "search_type": "annotation_name",
          "patterns": [
            "org.springframework.web.socket.WebSocketHttpHeaders.put:java.util.List(java.lang.String,java.util.List)",
            "org.springframework.web.socket.WebSocketHttpHeaders.putAll:void(org.springframework.http.HttpHeaders)"
          ]
        }
      }
    },
    "routing": {
      "handler_classes": {
        "annotations": {
          "type": "joern",
          "search_type": "annotation_name",
          "patterns": [
            "EnableWebSocket",
            "EnableWebSocketMessageBroker"
          ]
        }
      }
    }
  }
}