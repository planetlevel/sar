{
  "$schema": "../schema/framework-schema.json",
  "name": "LangChain Python",
  "languages": [
    "python"
  ],
  "extends": "python",
  "detection": {
    "dependencies": {
      "requirements.txt": [
        {
          "pattern": "langchain",
          "library": "LangChain"
        }
      ],
      "pyproject.toml": [
        {
          "pattern": "langchain",
          "library": "LangChain"
        }
      ]
    }
  },
  "architecture": {
    "integration": {
      "ai_services": {
        "type": "joern",
        "search_type": "method_signature",
        "signatures": [
          "langchain.py:<module>.ChatOpenAI.__init__.<returnValue>.predict",
          "langchain.py:<module>.LLM.__init__.<returnValue>.predict",
          "langchain.py:<module>.Chain.__init__.<returnValue>.run",
          "langchain.py:<module>.Chain.__init__.<returnValue>.invoke",
          "langchain.py:<module>.Embeddings.__init__.<returnValue>.embed_documents",
          "langchain.py:<module>.Embeddings.__init__.<returnValue>.embed_query"
        ]
      }
    }
  }
}