{
  "$schema": "../schema/framework-schema.json",
  "name": "Google Generative AI",
  "languages": [
    "python"
  ],
  "extends": "python",
  "detection": {
    "dependencies": {
      "requirements.txt": [
        {
          "pattern": "google-generativeai",
          "library": "Google Generative AI"
        }
      ],
      "pyproject.toml": [
        {
          "pattern": "google-generativeai",
          "library": "Google Generative AI"
        }
      ]
    }
  },
  "architecture": {
    "integration": {
      "ai_services": {
        "type": "joern",
        "search_type": "method_signature",
        "signatures": [
          "google.generativeai.py:<module>.GenerativeModel.__init__.<returnValue>.generate_content",
          "google.generativeai.py:<module>.ChatSession.__init__.<returnValue>.send_message"
        ]
      }
    }
  }
}