        {
          "artifact": "requests",
          "library": "Python Requests Library"
        }
      ]
    }
  },
  "architecture": {
    "integration": {
      "http_clients": [
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "src/requests/api.py:<module>.get",
          "description": "Sends HTTP GET request to specified URL"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "src/requests/api.py:<module>.post",
          "description": "Sends HTTP POST request with data/json payload"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "src/requests/api.py:<module>.put",
          "description": "Sends HTTP PUT request with data payload"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "src/requests/api.py:<module>.delete",
          "description": "Sends HTTP DELETE request"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "src/requests/api.py:<module>.head",
          "description": "Sends HTTP HEAD request"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "src/requests/api.py:<module>.patch",
          "description": "Sends HTTP PATCH request with data payload"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "src/requests/api.py:<module>.options",
          "description": "Sends HTTP OPTIONS request"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "src/requests/api.py:<module>.request",
          "description": "Sends generic HTTP request with specified method"
        }
      ]
    },
    "data_flow": {
      "serialization": [
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "src/requests/models.py:<module>.Response.json",
          "description": "Deserializes JSON response content"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "src/requests/models.py:<module>.Response.text",
          "description": "Decodes response content as text"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "src/requests/models.py:<module>.Response.content",
          "description": "Returns raw bytes content of response"
        }
      ],
      "input_sources": [
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "src/requests/models.py:<module>.Response.iter_content",
          "description": "Iterates over response content in chunks"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "src/requests/models.py:<module>.Response.iter_lines",
          "description": "Iterates over response content line by line"
        }
      ]
    },
    "presentation": {
      "headers": [
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "src/requests/models.py:<module>.PreparedRequest.prepare",
          "description": "Prepares request headers and data for transmission"
        }
      ]
    },
    "communication": {
      "http": [
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "src/requests/sessions.py:<module>.Session.send",
          "description": "Sends prepared request and returns response"
        },
        {
          "target": "joern",
          "search_type": "method_signature",
          "signature": "src/requests/sessions.py:<module>.Session.mount",
