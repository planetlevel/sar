    "database": {
      "ldap_queries": [
        {
          "target": "joern",
          "search_type": "annotation_name",
          "pattern": "org.springframework.security.kerberos.client.ldap.KerberosLdapContextSource.getDirContextInstance.PrivilegedAction$0.run:javax.naming.directory.DirContext():javax.naming.directory.DirContext()",
