{
  "$schema": "../schema/framework-schema.json",
  "name": "Anthropic Claude SDK",
  "languages": [
    "java"
  ],
  "extends": "java",
  "detection": {
    "dependencies": {
      "pom.xml": [
        {
          "artifact": "anthropic-java",
          "library": "Anthropic Claude SDK"
        }
      ],
      "build.gradle": [
        {
          "pattern": "com.anthropic",
          "library": "Anthropic"
        }
      ]
    }
  },
  "architecture": {
    "integration": {
      "ai_services": {
        "type": "joern",
        "search_type": "method_signature",
        "signatures": [
          "com.anthropic.client.AnthropicClient.messages:com.anthropic.models.Message(com.anthropic.models.MessageCreateParams)",
          "com.anthropic.client.AnthropicClient.messagesStream:java.util.stream.Stream(com.anthropic.models.MessageCreateParams)"
        ]
      }
    }
  }
}
